<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#D16A1B"></solid>
            <corners android:radius="50dp" />
            <stroke android:width="1dp" android:color="#FFFFFF" />
            <padding android:bottom="10dp" android:left="10dp" android:right="10dp" android:top="10dp" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient android:angle="270" android:endColor="#CC905C" android:startColor="#0178" />
            <stroke android:width="1dp" android:color="#883102" />
            <corners android:radius="50dp" />
            <padding android:bottom="20dp" android:left="10dp" android:right="10dp" android:top="10dp" />
        </shape>
    </item>
</selector>
